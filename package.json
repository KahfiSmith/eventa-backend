{
  "name": "eventa-backend",
  "version": "1.0.0",
  "description": "Local Event Discovery App Backend API",
  "main": "dist/service.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/service.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/service.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset"
  },
  "keywords": [
    "event",
    "discovery",
    "local",
    "tickets",
    "location",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
